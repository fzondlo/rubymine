"use this command to see all possible binginds
":actionlist

let mapleader = " "
map <leader>s :split<CR>
map <leader>v :vsplit<CR>
map <leader>b :action GotoDeclaration<CR>
map <leader>e :action RecentFiles<CR>
map <leader>w :action EditorSelectWord<CR>
map <leader>[ :action EditorCodeBlockStart<CR>
map <leader>] :action EditorCodeBlockEnd<CR>
map <leader>j :action AceAction<CR>
map <leader>q :action VimWindowClose<CR>

" window switching
map <leader>j :action VimWindowUp<CR>
map <leader>k :action VimWindowDown<CR>
map <leader>h :action VimWindowLeft<CR>
map <leader>l :action VimWindowRight<CR>

" DISABLED
